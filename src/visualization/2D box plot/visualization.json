{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "title": "params['title']",
    "description": "A 2D box plot.",
    "data": {
        "values": []
    },
    "mark": {
        "type": "boxplot",
        "extent": "params['extent']"
    },
    "encoding": {
        "x": {
            "field": "attributes['values'][0]",
            "type": "quantitative",
            "scale": {
                "zero": false
            }
        },
        "y": {
            "field": "attributes['categories'][0]",
            "type": "nominal",
            "scale": {
                "zero": false
            }
        },
         "color": {
            "field": "attributes['categories'][0]",
            "type": "nominal",
            "legend": null
        }
    }
}